{
    "name": "mtymek/modular-expressive",
    "description": "Provides integration between Zend Expressive and ZF2 modules",
    "license": "2-Clause BSD",
    "authors": [
        {
            "name": "Mateusz Tymek",
            "email": "mtymek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "zendframework/zend-config": "^2.5",
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-expressive": "~1.0-dev",
        "zendframework/zend-servicemanager": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "zendframework/zend-expressive-fastroute": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "ModularExpressive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModularExpressive\\Test\\": "test"
        }
    }
}
